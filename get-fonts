mkmdir -p C64_TrueType_v1.2.1-STYLE/fonts
 unzip C64_TrueType_v1.2.1-STYLE.zip C64_TrueType_v1.2.1-STYLE/fonts/C64_Pro_Mono-STYLE.ttf
mv C64_TrueType_v1.2.1-STYLE/fonts/C64_Pro_Mono-STYLE.ttf fonts/
rmdir C64_TrueType_v1.2.1-STYLE/fonts
rmdir C64_TrueType_v1.2.1-STYLE
